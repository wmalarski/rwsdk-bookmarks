{
  "$schema": "node_modules/wrangler/config-schema.json",
  "assets": {
    "binding": "ASSETS"
  },
  "compatibility_date": "2025-05-07",
  "compatibility_flags": ["nodejs_compat"],
  "d1_databases": [
    {
      "binding": "DB",
      "database_id": "rwsdk-bookmarks",
      "database_name": "rwsdk-bookmarks"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "SessionDurableObject",
        "name": "SESSION_DURABLE_OBJECT"
      }
    ]
  },
  "main": "src/worker.tsx",
  "migrations": [
    {
      "new_sqlite_classes": ["SessionDurableObject"],
      "tag": "v1"
    }
  ],
  "name": "rwsdk-bookmarks",
  "observability": {
    "enabled": true
  },
  "vars": {
    "WEBAUTHN_APP_NAME": "rwsdk-bookmarks"
  }
}
